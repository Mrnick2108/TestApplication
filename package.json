{
  "name": "test_site",
  "version": "1.0",
  "description": "Test electorn app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Test App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "release": "build"
  },
  "author": "Nick",
  "license": "MIT",
  "dependencies": {
    "electron": "^8.3.0",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "electron-packager": "^14.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mrnick2108/TestApplication.git/"
  },
      "publish": {
      "provider": "github",
      "releaseType": "release"
  },
  "build": {
      "win": {
      "publish": [
              "github"
          ]
      }
    }
}
